datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       String  @id @default(cuid())
  email    String  @unique
  password String
  plans    Plan[]
}

model Plan {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  sport     String
  goal      String
  age       Int
  plan      String
  userId    String?
  user      User?    @relation(fields: [userId], references: [id])
}