generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Vartotojo lentelÄ—, tinka registracijai, prisijungimui ir profilio redagavimui
model User {
  id           String   @id @default(uuid())
  name         String?
  email        String   @unique
  password     String
  goal         String?
  phone        String?
  dateOfBirth  DateTime?
  city         String?
  created_at   DateTime @default(now())
}
